{
  "name": "aiesec-ig-backend",
  "version": "1.0.0",
  "description": "Express service to fetch people and application data from the EXPA GraphQL API and persist to MongoDB.",
  "main": "src/server.js",
  "private": true,
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "sync": "node src/jobs/syncPeopleJob.js && node src/jobs/syncApplicationsJob.js",
    "sync:applications": "node src/jobs/syncApplicationsJob.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.5.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}
